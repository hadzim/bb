# Touchscreen lib
TSLIB_CONSOLEDEVICE=none
TSLIB_FBDEVICE=/dev/fb0
TSLIB_TSDEVICE=/dev/input/crystaltouch
TSLIB_CALIBFILE=/etc/pointercal
TSLIB_CONFFILE=/etc/ts.conf

# Hardware
TS_SYS_PATH="/sys/devices/platform/omap/omap2_mcspi.4/spi_master/spi4/spi4.0/"
TS_PRESSURETOUCH=15
TS_PRESSURERELEASE=8
TS_PERIOD=10
TS_SETCHARGETIME=1
